  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhta.mybatis.mapper.lhj.Admin_AccommodationsMapperlhj">
	<select id="accommList" resultType="com.jhta.project.vo.lhjcjy.AccommodationsVolhjcjy">
		select * from accommodations where aisdel is null
	</select>
	<select id="roomList" resultType="com.jhta.project.vo.lhjcjy.Room_InfoVolhjcjy">
