<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhta.mybatis.mapper.phj.BoardMapper_phj">
	<insert id="insertBoard" parameterType="com.jhta.project.vo.phj.BoardVo_phj">
		insert into board
		values(seq.board.nextval,#{bcate},#{btitle},#{bcontent},
		#{brdate},#{mid})
	</insert>
	<select id="selectBoardAll" resultType="com.jhta.project.vo.phj.BoardVo_phj">
		select * from board
	</select>
</mapper>